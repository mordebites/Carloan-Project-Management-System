<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.boundary.markup.SchermataModificaAgenzia">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Modifica agenzia">
         <font>
            <Font name="Helvetica" size="30.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="61.0" prefHeight="36.0" prefWidth="245.0" text="Compila i seguenti campi per modificare i dati dell'agenzia selezionata:" wrapText="true" />
      <Button fx:id="salva" layoutX="220.0" layoutY="260.0" mnemonicParsing="false" onAction="#onSalva" text="Salva" />
      <Button fx:id="annulla" layoutX="284.0" layoutY="260.0" mnemonicParsing="false" onAction="#onAnnulla" text="Annulla" />
      <GridPane layoutX="14.0" layoutY="110.0" prefHeight="123.0" prefWidth="330.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nome:" GridPane.rowIndex="1" />
            <Label text="Numero telefono:" GridPane.rowIndex="2" />
            <TextField fx:id="numTel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="nome" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Indirizzo:" GridPane.rowIndex="3" />
            <TextField fx:id="indirizzo" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="ID:" />
            <Label fx:id="id" disable="true" text="ID dell'agenzia selezionata" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
