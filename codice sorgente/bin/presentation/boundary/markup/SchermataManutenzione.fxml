<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="370.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.boundary.markup.SchermataManutenzione">
   <children>
      <Label layoutX="23.0" layoutY="22.0" text="Manutenzione">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="manut" layoutX="23.0" layoutY="113.0" prefHeight="200.0" prefWidth="402.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="45.0" text="ID" />
          <TableColumn fx:id="auto" prefWidth="51.0" text="Auto" />
            <TableColumn fx:id="tipoManut" prefWidth="106.0" text="TipoManut" />
            <TableColumn fx:id="dataFine" prefWidth="91.0" text="Data Inizio" />
            <TableColumn fx:id="dataInizio" prefWidth="87.0" text="Data Fine" />
        </columns>
      </TableView>
      <Button fx:id="aggiungi" layoutX="183.0" layoutY="75.0" mnemonicParsing="false" onAction="#onAggiungi" text="Aggiungi" />
      <Button fx:id="annulla" layoutX="315.0" layoutY="323.0" mnemonicParsing="false" onAction="#onAnnulla" prefHeight="25.0" prefWidth="73.0" text="Annulla" />
      <Button fx:id="modifica" layoutX="264.0" layoutY="75.0" mnemonicParsing="false" onAction="#onModifica" text="Modifica" />
      <Button fx:id="elimina" layoutX="342.0" layoutY="75.0" mnemonicParsing="false" onAction="#onElimina" prefHeight="25.0" prefWidth="73.0" text="Elimina" />
   </children>
</AnchorPane>
